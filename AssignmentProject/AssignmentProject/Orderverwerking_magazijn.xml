    <pous>
      <pou name="Orderverwerking_magazijn" pouType="program">
        <interface>
          <localVars>
            <variable name="currentState">
              <type>
                <derived name="Task_1" />
              </type>
              <initialValue>
                <simpleValue value="Task_1.NieuweOrderGaragist" />
              </initialValue>
            </variable>
            <variable name="Nieuwe_order">
              <type>
                <BOOL />
              </type>
              <initialValue>
                <simpleValue value="TRUE" />
              </initialValue>
            </variable>
            <variable name="Controleren_voorraad">
              <type>
                <INT />
              </type>
              <initialValue>
                <simpleValue value="0" />
              </initialValue>
            </variable>
            <variable name="Order_magazijn">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="Order_uitgeprint">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="Order_verzameld">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="Geannuleerd">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="Order_controle">
              <type>
                <INT />
              </type>
              <initialValue>
                <simpleValue value="0" />
              </initialValue>
            </variable>
            <variable name="Order_voltooid">
              <type>
                <BOOL />
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">CASE currentState OF
    Task_1.NieuweOrderGaragist:
        // Nieuwe bestelling van een garagist
        IF Nieuwe_order = TRUE THEN
            currentState := VoorraadAdministratief;
        END_IF;
		
	Task_1.VoorraadAdministratief:
		// Controleren of de voorraad klopt (Adminastratief)
		IF Controleren_voorraad = 1 THEN
			Order_magazijn := TRUE;
			Order_uitgeprint := TRUE;
			Order_verzameld := TRUE;
			Geannuleerd := FALSE;
			currentState := VerzamelingArtikelen;
		ELSIF Controleren_voorraad = 2 THEN
			Order_magazijn := FALSE;
			Order_uitgeprint := FALSE;
			Order_verzameld := FALSE;
			Geannuleerd := TRUE;
		END_IF
	
		
	Task_1.VerzamelingArtikelen:
	// Controleren of de verzamelde artikelen overeenkomen met de Orderverzamellijst
		IF Order_controle = 1 THEN
			Order_voltooid := TRUE;
			Geannuleerd := FALSE;
		ELSIF Order_controle = 2 THEN
			Order_controle := 0;
			Order_voltooid := FALSE;
			Geannuleerd := FALSE;
		ELSIF Order_controle = 3 THEN
			Order_voltooid := FALSE;
			Geannuleerd := TRUE;			
		END_IF
		
END_CASE;</xhtml>
          </ST>
        </body>
        <addData>
          <data name="http://www.3s-software.com/plcopenxml/interfaceasplaintext" handleUnknown="implementation">
            <InterfaceAsPlainText>
              <xhtml xmlns="http://www.w3.org/1999/xhtml">PROGRAM Orderverwerking_magazijn
VAR
	currentState : Task_1 := Task_1.NieuweOrderGaragist;   
	Nieuwe_order: BOOL := TRUE;
	Controleren_voorraad: INT := 0;
	Order_magazijn: BOOL;
	Order_uitgeprint: BOOL;
	Order_verzameld: BOOL; 
	Geannuleerd: BOOL;
	Order_controle: INT :=0;
	Order_voltooid: BOOL;
END_VAR
</xhtml>
            </InterfaceAsPlainText>
          </data>
